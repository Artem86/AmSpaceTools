<UserControl x:Class="AmSpaceTools.Views.MsWebLogin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AmSpaceTools.Views"
             xmlns:ext="clr-namespace:AmSpaceTools.Infrastructure.Extensions"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <WebBrowser Navigating="loginBrowser_Navigating" x:Name="loginBrowser" ext:WebBrowserExtensions.BindableSource="{Binding SourceUrl}" />
    </Grid>
</UserControl>
